class Solution {
    public void rotate(int[][] matrix) {
        int n=matrix.length;
        int[][] newmatrix=new int[n][n];
        for(int i=0;i<n;i++){
            int k=n-1;
            for(int j=0;j<n;j++){
                newmatrix[i][j]=matrix[k][i];
                k--;
            }
        }
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                matrix[i][j]=newmatrix[i][j];
            }
        }
    }
}
