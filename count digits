// User function Template for Java

class Solution {
    static int evenlyDivides(int n) {
        // code here
        int count=Count(n,0,n);
        return count;
    }
    static int Count(int n,int count, int num){
        if(n==0) return count;
        if(n%10!=0 && num%(n%10)==0) count++;
        return Count(n/10,count,num);
    }
}
